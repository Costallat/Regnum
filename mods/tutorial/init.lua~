--sword			
snappy  = {times={[1]=0.01, [2]=0.01, [3]=0.01}, uses=40960000, maxlevel=4},
 --axe                       
choppy  = {times={[1]=0.01, [2]=0.01, [3]=0.01}, uses=40960000, maxlevel=4},
--shovel			
crumbly = {times={[1]=0.01, [2]=0.01, [3]=0.01}, uses=40960000, maxlevel=4},
--pick			
cracky  = {times={[0]=0.01, [1]=0.01, [2]=0.01, [3]=0.01}, uses=40960000, maxlevel=4},
		},
		damage_groups = {fleshy=20}
	},
})

