		max_drop_level=3,
		groupcaps={
			choppy={times={[1]=0.01, [2]=0.01, [3]=0.01}, uses=1000, maxlevel=3},
			fleshy={times={[2]=0.01, [3]=0.01}, uses=1000, maxlevel=1}
		}
	},
})

minetest.register_craft({
        output = 'tutorial:axe_ice',
        recipe = {
                {'default:ice', 'default:ice', ''},
                {'default:ice', 'tutorial:redstone', ''},
                {'', 'tutorial:redstone', ''},
        }
})

