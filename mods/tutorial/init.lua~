minetest.register_abm({
	nodenames = {"tutorial:geschenk_wool1"},
	interval = 5,
	chance = 6,
	action = function(pos)
		minetest.env:add_node(pos, {name="tutorial:blue_wool_titan30"})
	end,
})

minetest.register_abm({
	nodenames = {"tutorial:geschenk_wool1"},
