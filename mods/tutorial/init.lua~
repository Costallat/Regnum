	description = "Ice Sword",
	inventory_image = "tutorial_tool_icesword.png",
	tool_capabilities = {
		full_punch_interval = 1.0,
		max_drop_level=3,
		groupcaps={
			fleshy={times={[2]=0.01, [3]=0.01}, uses=1000, maxlevel=3},
			snappy={times={[2]=0.01, [3]=0.01}, uses=1000, maxlevel=3},
			choppy={times={[3]=0.01}, uses=1000, maxlevel=3}
		}
	}
})

