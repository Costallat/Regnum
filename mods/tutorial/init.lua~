	type = "cooking",
	recipe = "tutorial:bottle12",
	output = "tutorial:bottleS1",
})

minetest.register_craft({
        output = 'tutorial:bottleS2',
        recipe = {
                {'tutorial:bottleS1', 'tutorial:bottleS1', ''},
                {'', '', ''},
                {'', '', ''},
        }
})

