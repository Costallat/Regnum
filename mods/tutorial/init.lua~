minetest.register_tool("tutorial:cloudbattleaxe_mega1", {
	description = "Cloudbattleaxe lv.1",
	inventory_image = "tutorial_cloudbattleaxe_mega1.png",
	tool_capabilities = {
		full_punch_interval = 2.0,
		max_drop_level=1,
		groupcaps={
--sword	                
choppy  = {times={[1]=0.01, [2]=0.01, [3]=0.01}, uses=10000, maxlevel=4},
--sword			
snappy  = {times={[1]=0.01, [2]=0.01, [3]=0.01}, uses=10000, maxlevel=4},
 --axe                       
choppy  = {times={[1]=0.01, [2]=0.01, [3]=0.01}, uses=10000, maxlevel=4},
--shovel			
crumbly = {times={[1]=0.01, [2]=0.01, [3]=0.01}, uses=10000, maxlevel=4},
--pick			
cracky  = {times={[0]=0.01, [1]=0.01, [2]=0.01, [3]=0.01}, uses=10000, maxlevel=4},
		},
		damage_groups = {fleshy=10},
	},
})

